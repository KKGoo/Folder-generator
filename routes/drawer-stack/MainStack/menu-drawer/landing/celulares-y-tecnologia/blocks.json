    }
  },
  "product-summary-image#productimageTech": {
    "props": {
      "blockClass": "productImageTech"
    }
  },
  "product-summary-name#productnameTech": {
    "props": {
      "blockClass": "productNameTech",
      "maxCharts": 15,
      "truncate": true
    }
  },
  "product-summary-price#productprice": {
    "props": {
      "blockClass": "productPrice",
      "deleteDecimals": true
    }
  },
  "product-summary.shelf#listTech": {
    "children": [
      "product-summary-image#productimageTech",
      "flex-layout.col#tech-product-tittlecontainer"
    ],
    "props": {
      "blockClass": "listTech",
      "horizontal": true,
      "redirectTo": "{link}"
    }
  },
