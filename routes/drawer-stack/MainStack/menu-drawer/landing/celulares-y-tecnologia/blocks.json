    "children": [
      "product-summary.shelf#listTech"
    ],
    "props": {
      "blockClass": "wrapperLg;mt_2;pt_2",
      "params": {
        "category": "celulares-y-telefonia",
        "department": "tecnologia"
      },
      "perPage": 9
    }
