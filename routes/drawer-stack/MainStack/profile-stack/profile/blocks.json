  "signout#log-out": {
    "children": [
      "icon#power-outline",
      "rich-text#log-out"
    ],
    "props": {
      "blockClass": "signoutButtonStyle",
      "content": "flex-layout.row#contentModalSignOut",
      "modalType": "acceptCancel",
      "mode": "onChange",
      "redirectTo": "/auth/login"
    }
  },
  "store-link#to-edit-profile": {
    "props": {
      "blockClass": "editProfileButtonStyles",
      "displayMode": "button",
      "label": "Editar",
      "leftIcon": "icon#edit-profile-button",
      "url": "/account/profile/edit"
    }
  },
  "store.profile": {
    "blocks": [
      "scroll-view#profile"
    ]
  }
